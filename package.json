{
  "name": "hag-community-project",
  "version": "1.0.4",
  "description": "Heroes and Generals community project",
  "scripts": {
    "build:assets": "npm run build --workspace=\"hagcp-assets\"",
    "build:canvas": "npm run build --workspace=\"hagcp-canvas\"",
    "build:networkClient": "npm run build --workspace=\"hagcp-network-client\"",
    "build:scripts": "npm run build --workspace=\"hagcp-scripts\"",
    "build:utils": "npm run build --workspace=\"hagcp-utils\"",
    "build:all": "npm run build:utils && npm run build:canvas && npm run build:networkClient && npm run build:assets && npm run build:scripts",
    "test": "npm run test --ws --if-present",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raouldeheer/Heroes-and-Generals-community-project.git"
  },
  "author": "Raoul de Heer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raouldeheer/Heroes-and-Generals-community-project/issues"
  },
  "homepage": "https://github.com/raouldeheer/Heroes-and-Generals-community-project#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/raouldeheer"
  },
  "workspaces": [
    "./packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0"
  }
}
