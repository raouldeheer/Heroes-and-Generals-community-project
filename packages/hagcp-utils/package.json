{
  "name": "hagcp-utils",
  "version": "1.0.9",
  "description": "This package is made for hagcp, the Hero and Generals Community Project. This package contains utils like a buffercursor, datastore and some byte functions. It also has some canvas utils",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev:build": "tsup --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raouldeheer/Heroes-and-Generals-community-project.git"
  },
  "author": "Raoul de Heer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raouldeheer/Heroes-and-Generals-community-project/issues"
  },
  "homepage": "https://github.com/raouldeheer/Heroes-and-Generals-community-project#readme",
  "dependencies": {
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  }
}
